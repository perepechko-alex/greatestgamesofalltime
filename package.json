{
  "name": "greatestgamesofalltime",
  "version": "1.0.0",
  "description": "Listing and determining the greatest video games of all time",
  "main": "api/index.js",
  "scripts": {
    "start-server": "node server.mjs",
    "dev": "APP_ENV=dev next dev",
    "install-sqlite": "npm install sqlite3 --build-from-source --sqlite=/opt/homebrew/opt/sqlite --save",
    "build": "next build ",
    "start": "next start",
    "lint": "next lint",
    "clean": "rm -rf out; rm -rf .next ",
    "export-static": "next export",
    "deploy": "npm run clean && npm run build && npm run export-static && aws s3 cp out s3://greatestgamesofalltime --recursive",
    "trim-extension": "for file in ./out/game/*.html; do mv \"$file\" \"${file%%.html}\"; done"
  },
  "author": "Alexander Perepechko",
  "license": "GNU AGPLv3",
  "dependencies": {
    "@babel/core": "^7.0.0",
    "@matejmazur/react-katex": "^3.1.3",
    "@material-ui/core": "^4.12.3",
    "@material-ui/icons": "^4.11.2",
    "csv-parse": "^4.14.2",
    "csv-stringify": "^5.5.3",
    "express": "^4.17.1",
    "katex": "^0.16.0",
    "material-ui-search-bar": "^1.0.0",
    "next": "^11.1.0",
    "postcss": "^8.2.15",
    "prop-types": "^15.7.2",
    "react": "^16.8.0",
    "react-dom": "^16.8.0",
    "react-icons": "^4.4.0",
    "sqlite3": "^5.0.8"
  },
  "devDependencies": {
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "prettier": "^2.2.1"
  }
}
